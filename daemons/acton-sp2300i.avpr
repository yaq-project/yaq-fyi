                "items": "double",
                "type": "array"
            }
        },
        "get_position": {
            "doc": "Get current daemon position.",
            "origin": "has-position",
            "request": [],
            "response": "double"
        },
        "get_side_entrance_slit": {
            "request": [],
            "response": [
                "int",
                "string"
            ]
        },
        "get_side_exit_slit": {
            "request": [],
            "response": [
                "int",
                "string"
            ]
